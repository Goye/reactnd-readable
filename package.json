{
  "name": "React-readable",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.17.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "moment": "^2.20.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier-stylelint": "^0.4.2",
    "promise": "8.0.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-router-redux-dom-link": "^0.3.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js"
  },
  "seedConfig": {
    "port": 3000
  },
  "engines": {
    "node": "6.x"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "space-before-blocks": "error",
      "semi": [
        "error",
        "always"
      ],
      "arrow-spacing": "error",
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": "error",
      "comma-style": "error",
      "eqeqeq": "error",
      "eol-last": "error",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "key-spacing": "error",
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-console": "off",
      "no-param-reassign": "error",
      "no-tabs": "error",
      "no-trailing-spaces": "error",
      "no-whitespace-before-property": "error",
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true
        }
      ],
      "react/display-name": "off",
      "react/prop-types": "off",
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "react/jsx-curly-spacing": [
        "error",
        "never"
      ],
      "react/jsx-boolean-value": [
        "error",
        "always"
      ],
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": true
        }
      ],
      "react/jsx-no-bind": [
        "error",
        {
          "ignoreRefs": false,
          "allowArrowFunctions": true,
          "allowBind": false
        }
      ],
      "react/require-render-return": [
        "error",
        "always"
      ],
      "react/no-is-mounted": "error",
      "react/jsx-wrap-multilines": [
        "error",
        {
          "return": true,
          "arrow": true
        }
      ],
      "no-var": "warn",
      "prettier/prettier": [
        "error",
        {
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "es5",
          "printWidth": 100,
          "parser": "babylon"
        }
      ]
    }
  }
}
